{
  "compilerOptions": {
    
    "declaration": false,
   
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ESnext",
		"module": "ESnext",
		"allowJs": true,
		"moduleResolution": "node",
		"lib": ["ESnext"],
    "sourceMap": false,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "strict": true,
    "skipLibCheck": true,
  
    "noImplicitAny": false,
 
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": false
  }
}
